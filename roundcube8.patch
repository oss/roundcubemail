+}
+
+rcube_mail_ui.prototype = {
+
+show_popupmenu: function(obj, refname, show, above)
+{
+  if (typeof show == 'undefined')
+    show = obj.is(':visible') ? false : true;
+  
+  var ref = rcube_find_object(refname);
+  if (show && ref) {
+    var pos = $(ref).offset();
+    obj.css({ left:pos.left, top:(pos.top + (above ? -obj.height() : ref.offsetHeight)) });
+  }
+  
+  obj[show?'show':'hide']();
+},
+
+show_markmenu: function(show)
+{
+  above = 0;
+  var obj = this.markmenu;
+  if (typeof show == 'undefined')
+    show = obj.is(':visible') ? false : true;
+  
+  var ref = "#markbutton";
+  if (show) {
+    var pos = $(ref).position();
+    var domref = document.getElementById("markbutton");
+    var offh = domref.offsetHeight;
