+++ roundcube_rutgers/skins/larry/rss/style.css	2012-11-29 15:02:34.000000000 -0500
@@ -0,0 +1,534 @@
+/* Open Systems Solutions */
+/*
+ * -moz and -webkit are used to define rounded corners 
+ * and are part of the CSS3 draft and will not validate 
+ * until CSS3 is ratified by W3C.  Because of this, there
+ * is no support for IE.
+ * 
+ * Version 1.1: July 21, 2009
+ * 
+ */
+
+
+body {
+	background: #eae7dd; 
+	font: 0.8em Verdana, sans-serif; 
+	color: #333;
+	margin: 0px;
+	}
+a {
+	color: #fff;
+	text-decoration: none;
+	}
+img {border: 0px;}
+
+/*canvas definition*/
+#main {
+	text-align: left;
+    margin: 5px;
+    margin-bottom: 50px;
+	min-width: 800px;
+}
+
+/*top banner*/
+#header {
+	background-image:url(newimages/spread.gif);
+	background-repeat: repeat;
+	min-width: 800px;
+	height: 134px;
+	}
+#seal{
+	position: absolute;
+	left: 50%;
+	margin-left: -262px;
+	height: 97px;
+	width: 524px;
+	background-image: url(newimages/header.gif);
+	}
+	
+/*Rutgers logo top left */
+#logo {
+	top: 20px; 
+	left: 1%; 
+	width: 223px;
+	height: 76px;
+	position: absolute;
+	float: right;
+
+	}
+	
+/* the horizontal menu starts here, menu in top right with drop */
+#listmenu {
+	top: 0; 
+	right: 1%; 
+	width: 230px;
+	height: 17px;
+	font-size: .9em;
+	color: #fff;
+	position: absolute;
+	float: right;
+	background-color: #1e1e1e;
+	-moz-border-radius-bottomleft: 5px;
+	-webkit-border-bottom-left-radius: 5px;
+	-moz-border-radius-bottomright: 5px;
+	-webkit-border-bottom-right-radius: px;
+	float: left; /*makes the div enclose the list */
+	}
+#listmenu ul {margin: 0 0 0 -40px;}/* indents ul from edge of container */
+#listmenu li:first-child {}
+#listmenu li {
+	float: left;	/* causes the list to align horizontally instead of stack */
+	position: relative; /* positioning context for the absolutely positioned drop-down */
+	list-style-type: none;	/* removes the bullet off each list item */
+	margin: 0;
+	}
+#listmenu a {
+	margin: 0; 
+	padding: 0;
+	display:block;
+	padding:0 6px; /*creates space each side of menu item's text */
+	text-decoration:none;	 /* removes the underlining of the link */
+	color:#fff;	/* sets the type color */
+	}
+#listmenu a:hover {
+	color:#f33;
+	}
+/* the horizontal menu ends here */
+
+/* ********** the drop-down starts here ************** */
+#listmenu ul li ul {
+	padding: 0;
+	margin: 0; /* prevents the TEMP value inheriting from the horiz menu - OK to remove if you remove TEMP above */
+	position: absolute; /* positions the drop-down ul in relation to its relatively positioned li parent */
+	width: 5em; /*sets the width of the menu - in combo with the li's 100% width, makes the menu stack*/
+	left: -1px; /*aligns the drop exactly under the menu */
+	z-index: 1;
+	background-color: #1e1e1e;
+	}
+#listmenu ul li ul li {
+	width: 50%; /* makes the list items fill the list container (ul) */
+	border-left: 1px solid #000; /*  three sides of each drop-down item */
+	border-right: 1px solid #000;
+	}
+/* make the drop-down display as the menu is rolled over */
+#listmenu ul li ul {display: none;} /* conceals the drop-down when menu not hovered */
+#listmenu ul li:hover ul {display: block; } /* shows the drop-down when the menu is hovered */
+
+/* END OF LIST-BASED MENU */
+
+/*Search box in top left */
+#search {
+	top: 30px; 
+	right: 1%;
+	width: 230px;
+	height: 25px;
+	position: absolute;
+	vertical-align: top;
+	text-align: right;
+	float: right;
+	z-index: 0;
+	}
+	
+#search fieldset
+{
+     border: 0px;
+}
+
+
+/*search text color*/
+#gs{
+	color: #fff;
+	font-size: .9em;
+}
+
+#go_button {
+	vertical-align: bottom;
+}
+
+/* Links below search box */
+#link_line {
+	top: 70px;  
+	right: 1%;
+	width: 230px;
+	height: 15px;
+	color: #fff;
+	position: absolute;
+	float: right;
+	font-size: .9em;
+	text-align: right;
+	}
+	
+/* Menu bar .. more like a twitterline at the moment*/
+#menu {
+	position: absolute;
+	top: 100px;
+	padding: 5px;
+	margin: 0;
+	font: 1.2em Verdana, sans-serif;
+/*	background: #1e1e1e; */
+	color: #fff;
+	min-width: 800px;
+	}
+	
+	
+/*Center Content -- creates the white space for the background see also body{background}*/
+#content {
+	-moz-border-radius: 10px;
+	-webkit-border-radius: 10px;
+	padding: 5px;
+    margin-top: 20px;
+    margin-left: 20px;
+	background: #fff;
+	max-width: 55%;
+	}
+	
+/* Box of text on left of page */
+/* Change width if you would like to occupy the whole white area (ugly) */
+#left {
+	width: 95%;
+	}
+	
+/* Surrounding div that contains right_top/middle/bottom */
+#right_boxes {
+	float: right;
+	margin-bottom: 50px;
+    }
+
+/* First box of three within right_boxes */
+#right_top {
+	width: 310px;
+	padding-bottom: 5px;
+	}
+#right_top a:link{
+	font-size: .8em;
+}
+#right_top a:visited{
+	color: #669;
+	font-size: .8em;
+}
+
+/* Second box of three within right_boxes */
+#right_middle{
+	width: 310px;
+	padding-bottom: 0px;
+	color: #000; 
+	}
+#right_middle a:link{
+	color: #669;
+	font-size: .8em;
+}
+#right_middle a:visited{
+	color: #669;
+	font-size: .8em;
+}
+
