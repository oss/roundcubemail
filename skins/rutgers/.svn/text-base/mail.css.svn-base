#listcontrolstop a.active:visited,
#mailboxcontrols a.active,
#mailboxcontrols a.active:active,
#mailboxcontrols a.active:visited,
td.formlinks a,
td.formlinks a:visited
{
  color: #0066CC;
} 

#listcontrolstop a.active:hover,
#mailboxcontrols a.active:hover
{
  text-decoration: underline;
} 
  
#listcontrolstop
{
  padding-right: 2em;
  display: inline;
  float: left;
  font-size: 11px;
}

#mailboxcontrols{
  text-align: center;
}


#countcontrolstop
{
